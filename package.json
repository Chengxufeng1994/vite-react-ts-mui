{
  "name": "vite-ts-react-mui-eslint-prettier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules",
    "commit": "cz",
    "dev": "vite",
    "dev:host": "npm run dev -- --host",
    "build": "cross-env NODE_ENV=production tsc -b && vite build",
    "build:dev": "cross-env NODE_ENV=development tsc -b && vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint --ignore-path .gitignore **/*.css",
    "stylelint:fix": "npm run stylelint -- --fix",
    "lint:all": "run-p lint stylelint",
    "format": "prettier -uw --ignore-path .gitignore --check .",
    "format:fix": "prettier -uw --ignore-path .gitignore --write .",
    "prepare": "husky",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "check-update": "npx npm-check-updates"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-charts": "^7.23.2",
    "axios": "^1.7.9",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-istanbul": "^2.1.8",
    "browserslist": "^4.24.3",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "npm-check-updates": "^17.1.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "start-server-and-test": "^2.0.9",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "terser": "^5.37.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
